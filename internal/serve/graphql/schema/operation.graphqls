type Operation {
  id: Int64!                   # Maps to int64
  operationType: OperationType!
  operationXdr: String!        # Maps to json:"operationXdr"
  ledgerCreatedAt: Time!
  ingestedAt: Time!
  
  # Relationships
  txHash: String!
  transaction: Transaction
  accounts: [Account!]!
  stateChanges: [StateChange!]!
}
